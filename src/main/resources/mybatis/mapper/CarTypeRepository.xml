<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.wghtstudio.insurance.dao.repository.CarTypeRepository">
    <select id="getCarTypeById" resultType="CarType">
        SELECT id, name
        FROM car_types
        WHERE id = #{id}
    </select>
</mapper>
